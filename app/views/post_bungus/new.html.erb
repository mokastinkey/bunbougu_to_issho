<p>文房具新規投稿画面</p>
<%= form_with model:@post_bungu, local:true do |f| %>
  <h4>文房具のキャッチフレーズ</h4>
  <%= f.text_field :catchphrase, autofocus: true %>
  <h4>画像</h4>
  <%= f.attachment_field :bungu_images_images, multiple: true %>
  <h4>使い心地や良い点など</h4>
  <%= f.text_area :thought %>
  <h4>メーカー</h4>
  <%= f.text_field :manufacturer %>
  <h4>値段</h4>
  <%= f.text_field :price %>
  <h4>購入場所</h4>
  <%= f.text_field :place %>
<% end %>
